CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE MOVIES(MID INTEGER NOT NULL PRIMARY KEY,TITLE VARCHAR(50),RELEASEYEAR INTEGER,CATEGORY1 VARCHAR(20),ENDMONTH INTEGER,ENDDAY INTEGER,ENDYEAR INTEGER,DESCRIPTION VARCHAR(200))
CREATE MEMORY TABLE USERS(UID INTEGER NOT NULL PRIMARY KEY,USERNAME VARCHAR(50),PASSWORD VARCHAR(20),AGE INTEGER,GENDER CHAR(1),EXPMONTH INTEGER,EXPDAY INTEGER,EXPYEAR INTEGER)
CREATE MEMORY TABLE VIEWS(UID INTEGER NOT NULL,MID INTEGER NOT NULL,RATING INTEGER,CONSTRAINT CONSTRAINT1 PRIMARY KEY(UID,MID),CONSTRAINT CONSTRAINT2 FOREIGN KEY(UID) REFERENCES USERS(UID) ON DELETE CASCADE,CONSTRAINT CONSTRAINT3 FOREIGN KEY(MID) REFERENCES MOVIES(MID) ON DELETE CASCADE)
CREATE USER GROUPD PASSWORD ""
GRANT DBA TO GROUPD
