<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="msms.comp3350.main.MainActivity">

    <Button
        android:id="@+id/MovieButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="179dp"
        android:text="Movies"
        android:onClick="buttonMoviesOnClick"/>

    <Button
        android:id="@+id/UsersButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/MovieButton"
        android:layout_below="@+id/MovieButton"
        android:text="Users"
        android:onClick="buttonUsersOnClick"/>
</RelativeLayout>
